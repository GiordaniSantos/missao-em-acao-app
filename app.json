{
  "expo": {
    "name": "Missão em Ação",
    "slug": "MissaoEmAcao",
    "scheme": "missaoemacao",
    "version": "1.0.1",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-asset"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f5d39"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0f5d39"
      },
      "package": "com.giordanisilveira.MissaoEmAcao"
    },
    "extra": {
      "eas": {
        "projectId": "07b4b464-85fe-4cb1-a9df-27f2f14b0991"
      }
    }
  }
}
